    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:01.038552Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:01.031947Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"from sklearn.model_selection import train_test_split\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"from sklearn.model_selection import train_test_split\\n\",\n",
    "    \"\\n\",\n",
    "    \"X = full_df[all_features]\\n\",\n",
    "    \"y = full_df[\\\"responsible_entity_id\\\"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Count the number of samples per class\\n\",\n",
    "    \"class_counts = y.value_counts()\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Select only classes with at least 2 samples\\n\",\n",
    "    \"valid_classes = class_counts[class_counts >= 2].index\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Filter X and y to keep only valid classes\\n\",\n",
    "    \"X_valid = X[y.isin(valid_classes)]\\n\",\n",
    "    \"y_valid = y[y.isin(valid_classes)]\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Now, you can safely stratify on y_valid\\n\",\n",
    "    \"X_train, X_val, y_train, y_val = train_test_split(\\n\",\n",
    "    \"    X_valid, y_valid,\\n\",\n",
    "    \"    test_size=0.2,\\n\",\n",
    "    \"    random_state=42,\\n\",\n",
    "    \"    stratify=y_valid\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
    "    \"X_train_filtered_new = X_train\\n\",\n",
    "    \"y_train_filtered_new = y_train\\n\",\n",
    "    \"\\n\",\n",
    "    \"X_val_filtered_new = X_val\\n\",\n",
    "    \"y_val_filtered_new = y_val\\n\",\n",
    "    \"\\n\"\n",
    "   ],\n",
    "   \"id\": \"a904a7ee87554eca\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 70\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:01.089511Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:01.086589Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"from sklearn.preprocessing import OneHotEncoder\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Create a text processing pipeline: TF-IDF vectorization + Dimensionality Reduction\\n\",\n",
    "    \"text_pipeline = Pipeline([\\n\",\n",
    "    \"    (\\\"tfidf\\\", TfidfVectorizer(\\n\",\n",
    "    \"        stop_words=german_stopwords,\\n\",\n",
    "    \"        max_features=10000,\\n\",\n",
    "    \"        ngram_range=(1, 2),\\n\",\n",
    "    \"        min_df=3,\\n\",\n",
    "    \"        max_df=0.9\\n\",\n",
    "    \"    )),\\n\",\n",
    "    \"    (\\\"svd\\\", TruncatedSVD(n_components=300, random_state=42))  # Reduce to 300 dimensions\\n\",\n",
    "    \"])\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Combine text and keyword features into a single preprocessing pipeline\\n\",\n",
    "    \"preprocessor = ColumnTransformer(\\n\",\n",
    "    \"    transformers=[\\n\",\n",
    "    \"        (\\\"text\\\", text_pipeline, \\\"description\\\"),\\n\",\n",
    "    \"        (\\\"keywords\\\", \\\"passthrough\\\", keyword_features),\\n\",\n",
    "    \"        (\\\"cat\\\", OneHotEncoder(handle_unknown=\\\"ignore\\\"), categorical_features), # onehot encoding\\n\",\n",
    "    \"    ]\\n\",\n",
    "    \")\"\n",
    "   ],\n",
    "   \"id\": \"353ceba880177f69\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 71\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:01.139570Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:01.137247Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Create full pipeline: preprocessing + classifier\\n\",\n",
    "    \"pipeline = Pipeline([\\n\",\n",
    "    \"    (\\\"preprocessor\\\", preprocessor),\\n\",\n",
    "    \"    (\\\"clf\\\", RandomForestClassifier(\\n\",\n",
    "    \"    n_estimators=100,\\n\",\n",
    "    \"    class_weight=\\\"balanced\\\",\\n\",\n",
    "    \"    random_state=42,\\n\",\n",
    "    \"    n_jobs=-1\\n\",\n",
    "    \"))\\n\",\n",
    "    \"])\"\n",
    "   ],\n",
    "   \"id\": \"995f9600f6d18d31\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 72\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:02.160756Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:01.187540Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Fit the model on the filtered training data\\n\",\n",
    "    \"pipeline.fit(X_train_filtered_new, y_train_filtered_new)\"\n",
