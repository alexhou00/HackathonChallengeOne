    "   },\n",
    "   \"source\": [\n",
    "    \"# Import pandas for data handling\\n\",\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Import NLTK stopwords for German text preprocessing\\n\",\n",
    "    \"import nltk\\n\",\n",
    "    \"from nltk.corpus import stopwords\\n\",\n",
    "    \"\\n\",\n",
    "    \"from sklearn.compose import ColumnTransformer\\n\",\n",
    "    \"from sklearn.pipeline import Pipeline\\n\",\n",
    "    \"from sklearn.feature_extraction.text import TfidfVectorizer\\n\",\n",
    "    \"from sklearn.decomposition import TruncatedSVD\\n\",\n",
    "    \"from sklearn.ensemble import RandomForestClassifier\\n\",\n",
    "    \"from sklearn.metrics import classification_report, accuracy_score\"\n",
    "   ],\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 64\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:00.764604Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:00.762323Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Download German stopwords if not already available\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    stopwords.words(\\\"german\\\")\\n\",\n",
    "    \"except LookupError:\\n\",\n",
    "    \"    nltk.download(\\\"stopwords\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Store German stopwords for use in TF-IDF vectorizer\\n\",\n",
    "    \"german_stopwords = stopwords.words(\\\"german\\\")\"\n",
    "   ],\n",
    "   \"id\": \"18c34bc5365d0b0e\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 65\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:00.818415Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:00.810261Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Load training and validation datasets from CSV files\\n\",\n",
    "    \"train_df = pd.read_csv('../data/challenge_1/train/classification_data.csv')\\n\",\n",
    "    \"val_df = pd.read_csv('../data/challenge_1/val/classification_data.csv')\\n\",\n",
    "    \"\\n\",\n",
    "    \"full_df = pd.concat([train_df, val_df], ignore_index=True)\"\n",
    "   ],\n",
    "   \"id\": \"6e6586bec9002e9b\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 66\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:00.866779Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:00.864499Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Features to use\\n\",\n",
    "    \"text_feature = \\\"description\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Define additional binary keyword indicator features\\n\",\n",
    "    \"keyword_features = [\\n\",\n",
    "    \"    \\\"has_verkehr_keywords\\\",\\n\",\n",
    "    \"    \\\"has_bildung_keywords\\\",\\n\",\n",
    "    \"    \\\"has_umwelt_keywords\\\",\\n\",\n",
    "    \"    \\\"has_gesundheit_keywords\\\",\\n\",\n",
    "    \"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"categorical_features = [\\\"state\\\", \\\"category\\\"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"all_features = [text_feature] + keyword_features + categorical_features\"\n",
    "   ],\n",
    "   \"id\": \"d3f602636dfc09fb\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 67\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:00.919730Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:00.916393Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Select features and labels from the training and validation datasets\\n\",\n",
    "    \"X_train = train_df[all_features]\\n\",\n",
    "    \"y_train = train_df[\\\"responsible_entity_id\\\"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"X_val = val_df[all_features]\\n\",\n",
    "    \"y_val = val_df[\\\"responsible_entity_id\\\"]\\n\"\n",
    "   ],\n",
    "   \"id\": \"30e4a561c5b57dbd\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 68\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-06-13T14:40:00.972753Z\",\n",
    "     \"start_time\": \"2025-06-13T14:40:00.970345Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# Limit the classification task to the top N most frequent categories\\n\",\n",
    "    \"#TOP_N = 20\\n\",\n",
    "    \"#top_classes = y_train.value_counts().nlargest(TOP_N).index\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Filter training data to only include top N classes\\n\",\n",
    "    \"#train_mask = y_train.isin(top_classes)\\n\",\n",
    "    \"#X_train_filtered = X_train[train_mask]\\n\",\n",
    "    \"#y_train_filtered = y_train[train_mask]\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Filter validation data to only include same top N classes\\n\",\n",
    "    \"#val_mask = y_val.isin(top_classes)\\n\",\n",
    "    \"#X_val_filtered = X_val[val_mask]\\n\",\n",
    "    \"#y_val_filtered = y_val[val_mask]\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Use all training and validation data as-is\\n\",\n",
    "    \"X_train_filtered = X_train\\n\",\n",
    "    \"y_train_filtered = y_train\\n\",\n",
    "    \"\\n\",\n",
    "    \"X_val_filtered = X_val\\n\",\n",
    "    \"y_val_filtered = y_val\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\"\n",
    "   ],\n",
    "   \"id\": \"e144a4c2bfbce57e\",\n",
